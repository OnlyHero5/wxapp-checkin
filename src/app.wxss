page {
  --bg: #0c1015;
  --surface: #121a24;
  --surface-elevated: #162131;
  --surface-soft: #0f151f;
  --border: rgba(148, 163, 184, 0.14);
  --text: #e7edf5;
  --muted: rgba(231, 237, 245, 0.6);
  --accent: #7bdcc0;
  --accent-strong: #5fc9ad;
  --accent-soft: rgba(123, 220, 192, 0.12);
  --danger: #f87171;
  --shadow-soft: 0 18rpx 30rpx rgba(0, 0, 0, 0.35);
  --shadow-tight: 0 12rpx 20rpx rgba(0, 0, 0, 0.3);

  --td-brand-color: var(--accent-strong);
  --td-brand-color-active: #49b49a;
  --td-brand-color-light: rgba(123, 220, 192, 0.18);
  --td-text-color-primary: var(--text);
  --td-text-color-secondary: rgba(231, 237, 245, 0.68);
  --td-text-color-placeholder: rgba(231, 237, 245, 0.45);
  --td-bg-color-container: var(--surface);
  --td-bg-color-component: var(--surface-elevated);
  --td-bg-color-component-active: #1a2536;
  --td-bg-color-secondarycontainer: #141e2d;
  --td-component-stroke: var(--border);
  --td-font-white-1: var(--text);
  --td-font-gray-1: rgba(231, 237, 245, 0.9);
  --td-font-gray-2: rgba(231, 237, 245, 0.65);
  --td-font-gray-3: rgba(231, 237, 245, 0.45);
  --td-cell-bg-color: transparent;
  --td-cell-border-color: var(--border);
  --td-cell-note-color: rgba(231, 237, 245, 0.6);
  --td-cell-title-color: var(--text);
  --td-button-default-bg-color: rgba(231, 237, 245, 0.06);
  --td-button-default-border-color: rgba(231, 237, 245, 0.18);
  --td-button-default-color: rgba(231, 237, 245, 0.9);
  --td-button-primary-bg-color: var(--accent-strong);
  --td-button-primary-border-color: var(--accent-strong);
  --td-button-primary-color: #0b1117;
  --td-button-primary-active-bg-color: #49b49a;
  --td-button-primary-active-border-color: #49b49a;
  --td-input-bg-color: var(--surface-elevated);
  --td-input-border-color: var(--border);
  --td-input-placeholder-text-color: rgba(231, 237, 245, 0.45);
  --td-input-default-text-color: var(--text);
  --td-success-color: var(--accent);
  --td-success-color-1: rgba(123, 220, 192, 0.16);
  --td-success-color-5: var(--accent);

  background-color: var(--bg);
  color: var(--text);
  font-family: "HarmonyOS Sans", "Source Han Sans SC", "PingFang SC", "Microsoft YaHei", sans-serif;
}

view, text {
  box-sizing: border-box;
}

button {
  border: none;
  line-height: 1.2;
  background: transparent;
}

button::after {
  border: none;
}

.page {
  min-height: 100vh;
  padding: 40rpx 30rpx 64rpx;
  position: relative;
  overflow: hidden;
  background: radial-gradient(circle at 12% 6%, rgba(123, 220, 192, 0.12), transparent 46%),
    radial-gradient(circle at 88% 0%, rgba(92, 110, 230, 0.08), transparent 42%),
    linear-gradient(180deg, rgba(13, 18, 24, 0.96), rgba(9, 12, 17, 1));
}

.header {
  margin-bottom: 28rpx;
  padding-top: 6rpx;
}

.content {
  position: relative;
  z-index: 1;
}

.eyebrow {
  font-size: 22rpx;
  text-transform: uppercase;
  letter-spacing: 4rpx;
  color: var(--muted);
}

.title {
  font-size: 40rpx;
  font-weight: 600;
  margin-top: 6rpx;
  letter-spacing: 2rpx;
  color: var(--text);
}

.title-line {
  width: 72rpx;
  height: 2rpx;
  margin-top: 10rpx;
  background: linear-gradient(90deg, rgba(123, 220, 192, 0.7), rgba(123, 220, 192, 0.05));
  opacity: 0.8;
}

.subtitle {
  font-size: 26rpx;
  color: var(--muted);
  margin-top: 12rpx;
}

.card {
  padding: 30rpx;
  border-radius: 20rpx;
  background: linear-gradient(180deg, rgba(19, 27, 38, 0.95), rgba(15, 21, 31, 0.98));
  border: 1rpx solid var(--border);
  box-shadow: var(--shadow-tight);
}

.info-card {
  padding: 30rpx;
}

.row {
  display: flex;
  align-items: center;
  justify-content: space-between;
  margin-bottom: 18rpx;
}

.row:last-child {
  margin-bottom: 0;
}

.label {
  font-size: 24rpx;
  color: var(--muted);
}

.value {
  font-size: 26rpx;
  color: var(--text);
  text-align: right;
  display: block;
  max-width: 68%;
  line-height: 1.4;
  word-break: break-all;
}

.info-row {
  display: flex;
  align-items: flex-start;
  justify-content: space-between;
  margin-bottom: 18rpx;
}

.info-row:last-child {
  margin-bottom: 0;
}

.divider {
  height: 1rpx;
  background: rgba(148, 163, 184, 0.12);
  margin: 12rpx 0 18rpx;
}

.hint {
  font-size: 22rpx;
  color: rgba(231, 237, 245, 0.55);
  margin-top: 16rpx;
}

.hint.subtle {
  color: rgba(231, 237, 245, 0.45);
  margin-top: 16rpx;
}

.btn-primary {
  margin-top: 24rpx;
  background: var(--accent-strong);
  color: #0b1117;
  font-weight: 600;
  border-radius: 999rpx;
  padding: 20rpx 32rpx;
  box-shadow: 0 12rpx 22rpx rgba(95, 201, 173, 0.25);
  transition: transform 0.15s ease, box-shadow 0.2s ease;
  min-height: 90rpx;
  letter-spacing: 2rpx;
}

.btn-primary:active {
  transform: translateY(2rpx) scale(0.98);
}

.btn-primary[disabled] {
  opacity: 0.4;
}

.btn-ghost {
  margin-top: 18rpx;
  background: rgba(231, 237, 245, 0.04);
  color: rgba(231, 237, 245, 0.9);
  border: 1rpx solid rgba(231, 237, 245, 0.16);
  border-radius: 999rpx;
  padding: 14rpx 28rpx;
  transition: transform 0.15s ease;
}

.btn-ghost:active {
  transform: translateY(2rpx) scale(0.98);
}

.input {
  background: rgba(22, 33, 49, 0.85);
  border-radius: 14rpx;
  padding: 18rpx 20rpx;
  margin-top: 18rpx;
  color: var(--text);
  border: 1rpx solid var(--border);
}

.input::placeholder {
  color: rgba(231, 237, 245, 0.45);
}

.tag {
  padding: 6rpx 16rpx;
  border-radius: 999rpx;
  font-size: 20rpx;
  background: rgba(123, 220, 192, 0.14);
  color: var(--accent);
  border: 1rpx solid rgba(123, 220, 192, 0.28);
}

.chip {
  padding: 4rpx 14rpx;
  border-radius: 999rpx;
  font-size: 20rpx;
  background: rgba(123, 220, 192, 0.14);
  color: var(--accent);
  border: 1rpx solid rgba(123, 220, 192, 0.28);
}

.network-banner {
  padding: 14rpx 20rpx;
  border-radius: 14rpx;
  background: rgba(248, 113, 113, 0.14);
  color: rgba(254, 202, 202, 0.92);
  margin-bottom: 20rpx;
  border: 1rpx solid rgba(248, 113, 113, 0.28);
}

.status-card {
  margin-top: 26rpx;
  padding: 24rpx;
  border-radius: 20rpx;
  background: rgba(20, 31, 45, 0.9);
  border: 1rpx solid rgba(148, 163, 184, 0.16);
}

.status-title {
  font-size: 28rpx;
  font-weight: 600;
  color: var(--text);
}

.status-message {
  font-size: 24rpx;
  color: var(--muted);
  margin-top: 10rpx;
}

.status-meta {
  font-size: 22rpx;
  color: rgba(231, 237, 245, 0.42);
  margin-top: 10rpx;
}

.success-overlay {
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  background: rgba(2, 6, 23, 0.78);
  display: flex;
  align-items: center;
  justify-content: center;
  z-index: 99;
  animation: fadeIn 0.25s ease-out;
  backdrop-filter: blur(6rpx);
}

.success-card {
  width: 520rpx;
  padding: 36rpx;
  border-radius: 28rpx;
  background: linear-gradient(180deg, rgba(19, 27, 38, 0.98), rgba(14, 20, 30, 0.98));
  border: 1rpx solid rgba(148, 163, 184, 0.16);
  text-align: center;
  animation: popIn 0.3s ease-out;
  box-shadow: 0 24rpx 40rpx rgba(0, 0, 0, 0.4);
}

.checkmark {
  width: 96rpx;
  height: 96rpx;
  border-radius: 50%;
  background: rgba(123, 220, 192, 0.14);
  border: 2rpx solid var(--accent);
  margin: 0 auto 18rpx;
  position: relative;
}

.checkmark::after {
  content: "";
  position: absolute;
  width: 34rpx;
  height: 18rpx;
  border-left: 6rpx solid var(--accent);
  border-bottom: 6rpx solid var(--accent);
  transform: rotate(-45deg);
  top: 32rpx;
  left: 28rpx;
}

@keyframes popIn {
  from {
    transform: scale(0.92);
    opacity: 0.6;
  }
  to {
    transform: scale(1);
    opacity: 1;
  }
}

@keyframes fadeIn {
  from {
    opacity: 0;
  }
  to {
    opacity: 1;
  }
}

.list-card {
  padding: 24rpx;
  border-radius: 22rpx;
  background: linear-gradient(180deg, rgba(18, 26, 36, 0.96), rgba(16, 22, 32, 0.98));
  border: 1rpx solid rgba(148, 163, 184, 0.14);
  margin-bottom: 18rpx;
  transition: transform 0.15s ease;
}

.list-card:active {
  transform: translateY(2rpx) scale(0.99);
}

.list-title {
  font-size: 28rpx;
  font-weight: 600;
  max-width: 75%;
  color: var(--text);
}

.list-meta {
  font-size: 22rpx;
  color: var(--muted);
  margin-top: 8rpx;
}

.empty {
  text-align: center;
  color: var(--muted);
  margin-top: 60rpx;
}
